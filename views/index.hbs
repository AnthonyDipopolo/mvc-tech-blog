<h1>See What Others Are Thinking</h1>
<!-- Output each user's email address, using a handlebars each loop -->
<ul>
  {{#each thoughts}}
    <li>
      <a href="/thought/{{this.id}}">
        <h2>{{title}}</h2>
      </a>
      <p>{{text}}</p><br>
      <p>Added By: {{user.username}}</p>
      <p>Created On: {{createdAt}}</p>
    </li>
  {{/each}}
</ul>
